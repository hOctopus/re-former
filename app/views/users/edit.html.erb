<h1>Edit User</h1>
<%= form_for(@user) do |f| %>
  <%= f.label :username, value: "User Name:" %>
  <%= f.text_field :username, value: "Example User" %>

  <%= f.label :email, value: "Email:" %>
  <%= f.email_field :email, value: "example@example.com" %>

  <%= f.label :password, value: "Password:" %>
  <%= f.password_field :password %>

  <%= f.submit "Update User" %>
<% end %>
